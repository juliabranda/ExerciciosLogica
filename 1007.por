programa
{
    funcao inicio()
    {
        // Declaração e inicialização das variáveis
        inteiro A = 5, B = 6, C = 7, D = 8
        inteiro DIFERENCA = 0

        // Cálculo da diferença
        DIFERENCA = (A * B) - (C * D)

        // Saída do resultado
        escreva("DIFERENCA = ", DIFERENCA, "\n")
    }
}
